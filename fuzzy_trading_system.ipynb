   "source": [
    "rules = [\n",
    "    FuzzyRule(\n",
    "        premise=[\n",
    "            (\"MACD\", \"High\"),\n",
    "            (\"AND\",\"RSI\", \"Low\"),\n",
    "            (\"AND\",\"STO\", \"Low\"),\n",
    "\n",
    "        ],\n",
    "        consequence=[(\"Decision\", \"Buy\")],\n",
    "    ),\n",
    "    FuzzyRule(\n",
    "        premise=[\n",
    "            (\"MACD\", \"Low\"),\n",
    "            (\"AND\",\"RSI\", \"High\"),\n",
    "            (\"AND\",\"STO\", \"High\"),\n",
    "\n",
    "        ],\n",
    "        consequence=[(\"Decision\", \"Buy\")],\n",
    "    ),\n",
    "    FuzzyRule(\n",
    "        premise=[\n",
    "            (\"MACD\", \"High\"),\n",
    "            (\"AND\",\"RSI\", \"Med\"),\n",
    "            (\"AND\",\"STO\", \"Med\"),\n",
    "            \n",
    "        ],\n",
    "        consequence=[(\"Decision\", \"Buy\")],\n",
    "    ),\n",
    "    FuzzyRule(\n",
    "        premise=[\n",
    "            (\"MACD\", \"Low\"),\n",
    "            (\"AND\",\"RSI\", \"Med\"),\n",
    "            (\"AND\",\"STO\", \"High\"),\n",
    "        ],\n",
    "        consequence=[(\"Decision\", \"Sell\")],\n",
    "    ),\n",
    "    FuzzyRule(\n",
    "        premise=[\n",
    "            (\"RSI\", \"High\"),\n",
    "            (\"AND\",\"STO\", \"Low\"),\n",
    "        ],\n",
    "        consequence=[(\"Decision\", \"Sell\")],\n",
    "    ),\n",
    "    FuzzyRule(\n",
    "        premise=[\n",
    "            (\"MACD\", \"Low\"),\n",
    "            (\"AND\",\"RSI\", \"High\"),\n",
    "            (\"AND\",\"STO\", \"High\"),\n",
    "        ],\n",
    "        consequence=[(\"Decision\", \"Sell\")])]"
   ]
