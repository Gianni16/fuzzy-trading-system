   "source": [
    "def Defuzzification(frame: pd.DataFrame ,fuzzy_parameters:list):\n",
    "    frame['Defuzzification'] = frame.apply(lambda x: model(\n",
    "    variables=fuzzy_variables(fuzzy_parameters),\n",
    "    rules=rules,\n",
    "    MACD = x['MACD'], \n",
    "    RSI=x['RSI'],\n",
    "    STO=x['STO'])[0]['Decision'], axis=1)\n",
    "    return frame"
   ]
  },
